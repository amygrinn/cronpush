{
  "name": "cron-push",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "run-p start-server start-client",
    "start:prod": "run-p start-server start-client:prod",
    "start-client": "vue-cli-service serve",
    "start-client:prod": "vue-cli-service serve --mode production",
    "start-server": "nodemon -w server -e ts --exec \"node -r ./node-registers server\"",
    "build": "vue-cli-service build",
    "serve": "pm2 restart cron-push",
    "test-client:unit": "vue-cli-service test:unit",
    "test-client:e2e": "vue-cli-service test:e2e",
    "test-server": "cross-env NODE_ENV=test mocha --exit -r node-registers server/**/*.spec.ts",
    "test-server:w": "yarn test-server -w --watch-files server/**/*.ts",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "bcrypt": "^3.0.7",
    "cron-parser": "^2.13.0",
    "cronstrue": "^1.88.0",
    "cross-env": "^7.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^7.0.1",
    "mysql2": "^2.1.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "sequelize": "^5.21.3",
    "sequelize-noupdate-attributes": "^1.0.0",
    "supertest": "^4.0.2",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "web-push": "^3.4.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/chai": "^4.2.5",
    "@types/express": "^4.17.2",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/luxon": "^1.21.0",
    "@types/mocha": "^5.2.4",
    "@types/multer": "^1.3.10",
    "@types/passport": "^1.0.2",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/sequelize": "^4.28.8",
    "@types/supertest": "^2.0.8",
    "@types/web-push": "^3.3.0",
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-e2e-cypress": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-plugin-typescript": "^4.1.0",
    "@vue/cli-plugin-unit-mocha": "^4.1.0",
    "@vue/cli-plugin-vuex": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "axios": "^0.19.0",
    "bootstrap": "^4.4.1",
    "bootstrap-vue": "^2.1.0",
    "chai": "^4.2.0",
    "core-js": "^3.4.5",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "luxon": "^1.22.0",
    "moment-locales-webpack-plugin": "^1.1.2",
    "node-sass": "^4.12.0",
    "nodemon": "^2.0.1",
